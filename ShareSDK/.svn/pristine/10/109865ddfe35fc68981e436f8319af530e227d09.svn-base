<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="bottom"
    android:background="@color/white"
    android:orientation="vertical"
    android:paddingTop="@dimen/dimens15">

  <RelativeLayout
      android:id="@+id/share_item_relativelayout"
      android:layout_width="match_parent"
      android:layout_height="230dp"
      >

  <com.het.share.widget.ViewFlow
      android:id="@+id/share_viewpager"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:scrollbars="none"
      android:background="@color/white"
      app:sidebuffer="2"
      />

    <com.het.share.widget.CircleFlowIndicator
        android:id="@+id/share_flow_indicator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:padding="4dip"
        app:fadeOut="1000"
        app:inactiveType="fill"
        app:flowradius="3dp"
        app:activeColor="@color/gray"
        app:inactiveColor="@color/darkgray"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="5dp"
        />

  </RelativeLayout>

    <View
        style="@style/horizonal_diliver"
         />

    <Button
        android:id="@+id/cancel_share"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimens55"
        android:background="@drawable/bg_color_selector"
        android:text="取消"
        android:textColor="@color/textcolor_36"
        android:textSize="@dimen/textSize16" />

</LinearLayout>